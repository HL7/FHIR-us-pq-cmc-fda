  "name": "ImpuritySubstance",
  "title": "Drug Substance Impurity",
  "status": "active",
  "description": "Any component of the drug substance that is not the chemical entity for product composition.",
  "fhirVersion": "5.0.0",
  "kind": "resource",
  "abstract": false,
  "type": "SubstanceDefinition",
